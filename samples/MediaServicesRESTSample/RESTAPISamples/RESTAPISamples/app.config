<?xml version="1.0"?>
<configuration>

  <appSettings>

    <add key="accessControlURI" value="https://wamsprodglobal001acs.accesscontrol.windows.net/v2/OAuth2-13" />
    <add key="serviceURI" value="https://media.windows.net/API/" />
    <add key="scope" value="WindowsAzureMediaServices" />

    <add key="accountName" value="Your-WAMS-Account-Name" />
    <add key="accountKey" value="Your-WAMS-Account-Key" />

    <add key="WamsStorageAccountName" value="Your-Storage-Account-Name"/>
    <add key="WamsStorageAccountKey" value="Your-Storage-Account-Key"/>

  </appSettings>


  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
</configuration>
